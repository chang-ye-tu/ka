
psi[4]*S_g_er_e+K_g_er_e*(psi[1]*gamma_er*(rho+delta)^2/2+psi[2]*gamma_er*(gamma_ir*rho+gamma_il*rho-delta*gamma_ir+delta*gamma_il)/(2*gamma_il*gamma_ir));
psi[3]*S_g_el_e+K_g_el_e*(psi[2]*gamma_el*(gamma_ir*rho+gamma_il*rho+delta*gamma_ir-delta*gamma_il)/(2*gamma_il*gamma_ir)+psi[1]*gamma_el*(rho-delta)*(rho+delta)/2);
