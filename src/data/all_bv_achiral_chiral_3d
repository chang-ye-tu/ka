
v = delta*(-gamma_ir*psi2*(M_g_ir-I)+gamma_il*psi1*(M_g_il-I)+N_g_ir*psi2+N_g_il*psi1)/2-((psi2*z12+psi1*z11)*(k_e*(I+M_e)+N_e)+(psi2*z22+psi1*z21)*(N_e-k_e*(I+M_e)))/2;
