
v = delta*(-k_i*psi2*(M_i-I)+k_i*psi1*(M_i-I)+N_i*psi2+N_i*psi1)/2-((psi2*z22+psi1*z21)*(N_g_er-gamma_er*(I+M_g_er))+(psi2*z12+psi1*z11)*(gamma_el*(I+M_g_el)+N_g_el))/2;
